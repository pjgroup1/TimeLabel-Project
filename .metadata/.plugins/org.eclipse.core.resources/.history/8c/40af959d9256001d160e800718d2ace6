package com.java.team.dto;

import lombok.Data;

@Data
public class ProductUpdateDto {

	private String itemName; // 상품명
	private int price; // 상품가격
	private String info; // 상품 정보
	private int quantity; // 상품 수량
	private String mainImage; // 상품 대표 이미지
	private String image1; // 상품 대표 이미지
	private String image2; // 상품 대표 이미지
	private String image3; // 상품 대표 이미지
	public ProductUpdateDto() {
		
	}
	public ProductUpdateDto(String itemName, int price, String info, int quantity, String mainImage, String image1,
			String image2, String image3) {
		super();
		this.itemName = itemName;
		this.price = price;
		this.info = info;
		this.quantity = quantity;
		this.mainImage = mainImage;
		this.image1 = image1;
		this.image2 = image2;
		this.image3 = image3;
	}
	
	
}
